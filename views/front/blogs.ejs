<%- include('front-header') %>


    <div class="heder-space-strip">
    </div>
    <section class=" sec-faq blog-sec-new">
        <div class="container">
            <div class="row">
                <div class="col-md-12 mb-5">
                    <h3 class="main-heading-index-new wow slideInLeft"><span>Blogs</span><b>Blogs</b></h3>
                </div>
            </div>
            <div class="row wow slideInUp">
                <% var i=1; if(blogs.length> 0){
                    blogs.forEach(function(blog){
                    %>
                    <div class="col-md-4">
                        <a href="">
                            <div class="team-box-new">
                                <img src="/home/blog/<%=blog.blogimage%>" style='width:800; height:400'
                                    class="img-fluid">
                                <p><small>

                                    </small></p>
                                <h5 class="sub-heading">
                                    <%-blog.title%>
                                </h5>
                                <h5>
                                    <%-blog.created_at%>
                                </h5>
                                <p>
                                    <%-blog.subheading%>
                                        <p>
                                            <a href="/blog-detail?id=<%=blog._id%>">View More</a>
                                        </p>
                            </div>
                        </a>
                    </div>
                    <% i++; }) } %>
            </div>
        </div>
    </section>


    <%- include('front-footer') %>
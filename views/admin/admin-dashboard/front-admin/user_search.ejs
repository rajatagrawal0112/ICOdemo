<% var i=1; if(user_details.length > 0){  
   user_details.forEach(function(details){


   %>

   <tr>
     <td><%- i %></td>
     
     <td><%- details.name%></td>
    <%if(details.profile_image){%>
       <td> <img src="/upload_user_profile/<%=details.profile_image%>" style="width: 50px;height: 50px;"></td>
       <%}else{%>
        <td><img src="admin_assets/images/faces-clipart/pic-1.png" style="width: 50px;height: 50px;"></td>
        <%}%>
     <td><%- details.email%></td>
     
     <td><%- details.wallet_address%></td>
     
     <td><%- details.status%>
       <%if(details.status == "active"){%>
         <a href="/deactiveUser?id=<%=details.email%>">Deactive</a>
      <%}else{%>
         <a href="/activeUser?id=<%=details.email%>">Active</a>
      <%}%></td>
      <td><a href="/edit-user?id=<%=details.email%>">Change Password</a></td>
     <td><%- details.created_at%></td>
     
     
   </tr>
  
      <% i++; }) }  %>